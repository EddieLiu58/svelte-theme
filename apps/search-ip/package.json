{
	"name": "ip",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"build:dev": "APP_ENV=development vite build",
		"build:prod": "APP_ENV=production vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"gh-pages": "vite build && npx gh-pages -d build"
	},
	"devDependencies": {},
	"type": "module",
	"dependencies": {
		"@auth/sveltekit": "^0.3.11",
		"@fontsource/noto-sans-tc": "^5.0.5",
		"@fontsource/noto-serif-tc": "^5.0.7",
		"@splidejs/svelte-splide": "^0.2.9",
		"@svelte-web-fonts/google": "^1.0.4",
		"@sveltejs/adapter-static": "^2.0.3",
		"@svelteuidev/composables": "^0.14.0",
		"@svelteuidev/core": "^0.14.0",
		"@vitejs/plugin-basic-ssl": "^1.0.1",
		"axios": "^1.5.1",
		"gh-pages": "^6.0.0",
		"postcss-import": "^15.1.0",
		"siema": "^1.5.1",
		"svelte-carousel": "^1.0.25",
		"svelte-geolocation": "^0.3.0",
		"svelte-headroom": "^2.2.1",
		"svelte-infinite-loading": "^1.3.8",
		"svelte-infinite-scroll": "^2.0.1",
		"svelte-meta-tags": "^3.0.4"
	}
}
